# Test Environment Configuration
# Copy this file to .env.test and fill in your values

# Base URLs
PLAYWRIGHT_TEST_BASE_URL=http://localhost:3000
STAGING_URL=https://staging.ecosystem-app.com

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key

# Cognito Configuration
TEST_COGNITO_USER_POOL_ID=your_user_pool_id
TEST_COGNITO_CLIENT_ID=your_client_id
TEST_COGNITO_REGION=us-east-1

# DynamoDB Configuration
TEST_DYNAMODB_TABLE_PREFIX=staging
DYNAMODB_USER_TABLE=User-staging
DYNAMODB_PROVIDER_PROFILE_TABLE=ProviderProfile-staging
DYNAMODB_BOOKING_TABLE=Booking-staging
DYNAMODB_SERVICE_TABLE=Service-staging

# S3 Configuration
S3_BUCKET_NAME=ecosystem-storage-staging

# AWS Payment Test Configuration
AWS_PAYMENT_TEST_KEY_ID=your_test_payment_key
AWS_PAYMENT_TEST_ACCOUNT_ID=your_test_account

# Test Configuration
TEST_TIMEOUT=30000
TEST_RETRY_COUNT=1
TEST_PARALLEL_WORKERS=4

# Slack Webhook (for CI notifications)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL